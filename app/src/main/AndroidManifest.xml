<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.noteyourday">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/diaryicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/diaryicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DisplayYourDay">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.noteyourday.DisplayYourDay" />
        </activity>
        <activity android:name=".MyDays">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.noteyourday.MyDays" />
        </activity>
        <activity android:name=".MyDiaryFeeling">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.noteyourday.MyDiaryFeeling" />
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>